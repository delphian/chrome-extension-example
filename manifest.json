{
  "manifest_version": 2,
  "name": "Chrome Extension Example",
  "version": "0.1.0",
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "icon-19.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "jquery.min.js",
        "content-script.js"
      ]
    }
  ],
  "icons": {
    "19": "icon-19.png",
    "48": "icon-48.png"
  }
}
